		<label>dew leftovers</label>
		<statBases>
			<Beauty>-5</Beauty>
			<Cleanliness>-2</Cleanliness>
		</statBases>
		<graphicData>
			<texPath>Things/Filth/LiquidFuel</texPath>
			<color>(138, 43, 226, 100)</color>
		</graphicData>
		<!-- <pathCost>5</pathCost> -->
